h1= subtitle
div.main-cont(layout='column')
	each thing in main
		p !{thing}
	if links
		div(flex)
			each link in links
				md-button.md-raised.md-primary.md-indigo-theme.collapse-button(ng-init=link.open + 'Collapse=true', ng-click=link.open + 'Collapse = !' + link.open + 'Collapse')= link.text
			each link in links
				div.collapse-section(collapse=link.open + 'Collapse' id=link.open)
					javascript-build(js-page=link.open)